		"@prisma/client": "^6.0.1",
		"@react-email/components": "0.0.30",
		"axios": "^1.4.0",
		"blurhash": "^2.0.5",
		"canvas": "^2.11.2",
		"cors": "^2.8.5",
		"dav-js": "git+https://github.com/dav-apps/dav-js.git#v2.6.3",
		"express": "^4.18.2",
		"graphql": "^16.7.1",
		"graphql-request": "^6.1.0",
		"node-html-parser": "^6.1.12",
		"nodemon": "^2.0.22",
		"pdfjs-dist": "^4.0.379",
		"react": "18.3.1",
