{
	"name": "pocketlib-api",
	"version": "0.1.0",
	"main": "dist/server.js",
	"type": "module",
	"scripts": {
		"start": "node dist/server.js",
		"dev": "concurrently \"npm:*:watch\"",
		"dev:email": "email dev --dir src/emails",
